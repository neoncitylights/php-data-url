<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
	configVersion="3"
	xmlns="https://www.phpdoc.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<title>php-data-url</title>
	<!-- <paths>
		<output>docs/build</output>
		<cache>docs/cache</cache>
	</paths> -->
	<version number="3.0.0">
		<api format="php">
			<default-package-name>php-data-url</default-package-name>
			<ignore hidden="true" symlinks="true">
				<path>tests/**/*</path>
				<path>vendor/**/*</path>
				<path>.history</path>
			</ignore>
			<extensions>
				<extension>php</extension>
			</extensions>
			<include-source>true</include-source>
			<markers>
				<marker>TODO</marker>
				<marker>FIXME</marker>
			</markers>
		</api>
	</version>
</phpdocumentor>
